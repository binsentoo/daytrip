{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tailwindcss -i src/css/input.css -o src/css/output.css --minify && vite build",
    "serve": "vite preview",
    "tailwind-watch": "tailwindcss -i src/css/input.css -o src/css/output.css --watch",
    "tailwind-build": "tailwindcss -i src/css/input.css -o src/css/output.css --minify"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.1.2",
    "axios": "^1.7.7",
    "core-js": "^3.8.3",
    "vite": "^5.4.1",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
